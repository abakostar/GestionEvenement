<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity InscriptionActivite.
    -->
    <changeSet id="20200930170803-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="activite_id"
                                 baseTableName="inscription_activite"
                                 constraintName="fk_inscription_activite_activite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activite"/>

        <addForeignKeyConstraint baseColumnNames="participant_id"
                                 baseTableName="inscription_activite"
                                 constraintName="fk_inscription_activite_participant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="participant"/>

    </changeSet>
</databaseChangeLog>
